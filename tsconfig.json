{
  "include": ["src/**/*"],
  "compilerOptions": {
    "lib": ["ESNext", "DOM.Iterable"],
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx"
  }
}
